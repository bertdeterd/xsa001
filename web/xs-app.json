{
	"welcomeFile": "index.html",
	"authenticationMethod": "route",

	"routes": [

		{
			"source": "/api/(.*)",
			"destination": "nodejs",
			"csrfProtection": false,
			"authenticationType": "xsuaa"
		},

		{
			"source": "(.*)(.xsjs)",
			"destination": "nodejs",
			"csrfProtection": false,
			"authenticationType": "xsuaa"
		}, 
		
		{
			"source": "(.*)(.xsodata)",
			"destination": "nodejs",
			"authenticationType": "xsuaa"
		}

	]

}